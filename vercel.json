{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs20.x"
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_WORKER_API_KEY": "@VITE_WORKER_API_KEY"
  },
  "build": {
    "env": {
      "VITE_WORKER_API_KEY": "@VITE_WORKER_API_KEY"
    }
  },
  "framework": "vite",
  "regions": ["iad1"],
  "github": {
    "enabled": true,
    "autoJobCancelation": true,
    "silent": false
  }
}
